{
    "name": "sonsofphp/event-sourcing",
    "type": "library",
    "description": "Event Sourcing library",
    "keywords": [
        "event-sourcing"
    ],
    "homepage": "https://github.com/SonsOfPHP/event-sourcing",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Estes",
            "email": "joshua@sonsofphp.com"
        }
    ],
    "support": {
        "issues": "https://github.com/SonsOfPHP/sonsofphp/issues",
        "forum": "https://github.com/orgs/SonsOfPHP/discussions",
        "docs": "https://docs.sonsofphp.com"
    },
    "autoload": {
        "psr-4": {
            "SonsOfPHP\\Component\\EventSourcing\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "doctrine/dbal": "^3.3",
        "sonsofphp/event-dispatcher": "^0.2@dev",
        "symfony/uid": "^5.4|^6.0"
    },
    "require-dev": {
        "symfony/http-foundation": "^5.4",
        "symfony/messenger": "^5.4|^6.0",
        "symfony/security": "^4.4"
    },
    "suggest": {
        "symfony/messenger": "Allows you to handle events asynchronously"
    },
    "extra": {
        "sort-packages": true,
        "branch-alias": {
            "dev-main": "0.2.x-dev"
        }
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/JoshuaEstes"
        }
    ]
}